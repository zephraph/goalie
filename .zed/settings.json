{
  "extensions": {
    "biome": false
  },
  "languages": {
    "TypeScript": {
      "language_servers": ["deno", "!typescript-language-server"],
      "formatter": "language_server"
    },
    "TSX": {
      "language_servers": ["deno", "!typescript-language-server"],
      "formatter": "language_server"
    },
    "JavaScript": {
      "language_servers": ["deno", "!typescript-language-server"],
      "formatter": "language_server"
    },
    "JSX": {
      "language_servers": ["deno", "!typescript-language-server"],
      "formatter": "language_server"
    }
  },
  "lsp": {
    "deno": {
      "settings": {
        "deno": {
          "enable": true,
          "lint": true,
          "unstable": false,
          "config": "./deno.json",
          "importMap": null,
          "suggest": {
            "autoImports": true,
            "completeFunctionCalls": true,
            "names": true,
            "paths": true
          },
          "codeLens": {
            "implementations": true,
            "references": true,
            "referencesAllFunctions": false
          }
        }
      }
    }
  }
}
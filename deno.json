{
  "name": "goalie",
  "version": "0.1.0",
  "exports": "./src/main.ts",
  "tasks": {
    "dev": "deno run --allow-all --watch src/main.ts",
    "build": "deno compile --allow-all --output ./bin/goalie src/main.ts",
    "start": "deno run --allow-all src/main.ts"
  },
  "imports": {
    "commander": "npm:commander@^12.0.0",
    "ink": "npm:ink@^5.0.1",
    "react": "npm:react@^18.3.1",
    "yaml": "npm:yaml@^2.3.4",
    "@std/path": "jsr:@std/path@^1.0.2",
    "@std/fs": "jsr:@std/fs@^1.0.1",
    "@std/assert": "jsr:@std/assert@^1.0.2"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:react@^18.3",
    "jsxImportSourceTypes": "npm:@types/react@^18.3",
    "strict": false
  },
  "nodeModulesDir": "auto",
  "lock": false
}
